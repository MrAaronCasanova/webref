              teamNames; // ['Jill', 'Dave', 'Alex', 'Amanda', 'Bill']

              // --------------------------------------------

              // Symbol Iterator
              // a tool that teaches objects how to respond to the 'for of loop'

              // Time to refactor the above code

              // using the [Symbol.iterator] key to tell the 'for of loop' how to iterate of the object

              const testingTeam = {
                lead: 'Amanda',
                tester: 'Bill',
                [Symbol.iterator]: function* () {
                  yield this.lead;
