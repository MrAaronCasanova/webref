                [Symbol.iterator]: function* () {
                  yield this.lead;
                  yield this.tester;
                },
              };

              const engineeringTeam = {
                testingTeam,
                size: 3,
                department: 'Engineering',
                lead: 'Jill',
                manager: 'Alex',
                engineer: 'Dave',
                [Symbol.iterator]: function* () {
                  yield this.lead;
                  yield this.manager;
                  yield this.engineer;
                  yield* this.testingTeam;
                },
              };

              const teamNames = [];
              for (let name of engineeringTeam) {
                // name refers to the value following each yield
                teamNames.push(name);
              };

              teamNames; // ['Jill', 'Alex', 'Dave', 'Amanda', 'Bill']

              // --------------------------------------------

              // Array Helpers do not work with Generators
              // we can not 'yield' from within array helpers or callbacks of any sort


              class Comment {
                constructor(content, children = []) {
                  this.content = content;
                  this.children = children;
                }

                *[Symbol.iterator]() {
                  yield this.content;
                  for (let child of this.children) {
                    // go into every child and find all the yield values within
                    yield* child;
                  }
                }
              };

              const rootCommentChildren = [
                new Comment('fantastic', [new Comment('i disagree'), new Comment('how come?') ]),
                new Comment('well said'),
                new Comment('more please'),
              ];

              const rootComment = new Comment('great post!', rootCommentChildren);

              const values = [];

              for (let value of rootComment) {
                values.push(value);
              };

              values; // ['great post!', 'fantastic', 'i disagree', 'how come?', 'well said', 'more please']
            </code>
